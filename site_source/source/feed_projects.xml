---
use: ["projects"]
permalink: feed.xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title><![CDATA[{{ site.title }} - Projets]]></title>
    <link href="{{ site.url }}/feed.xml" rel="self"/>
    <link href="{{ site.url }}/"/>
    <updated>{{ site.calculated_date | date('c') }}</updated>
    <id>{{ site.url }}/</id>
    {% if site.name or site.email %}
        <author>
            {% if site.name %}<name><![CDATA[{{ site.name }}]]></name>{% endif %}
            {% if site.email %}<email><![CDATA[{{ site.email }}]]></email>{% endif %}
        </author>
    {% endif %}
    <generator uri="http://sculpin.io/">Sculpin</generator>

    {% for post in data.projects %}
        <entry>
            <title type="html"><![CDATA[{{ post.title }}]]></title>
            <link href="{{ site.url }}{{ post.url }}"/>
            <updated>{{ post.date|date('c') }}</updated>
            <id>{{ site.url }}{{ post.url }}</id>
            <content type="html"><![CDATA[{{ post.blocks.content|raw }}]]></content>
        </entry>
    {% endfor %}

</feed>